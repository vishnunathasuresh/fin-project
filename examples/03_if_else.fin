# Test 3: If/else statements
# Expected output:
#   x is greater than 5
#   a equals b
#   Value is non-zero

set x 10
set a 5
set b 5
set val 1

if $x > 5
    echo "x is greater than 5"
else
    echo "x is not greater than 5"
end

if $a == $b
    echo "a equals b"
else
    echo "a does not equal b"
end

if $val != 0
    echo "Value is non-zero"
end
