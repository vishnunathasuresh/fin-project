# Test 9: Nested control structures
# Expected output:
#   i=1, j=1
#   i=1, j=2
#   i=2, j=1
#   i=2, j=2
#   i=3, j=1
#   i=3, j=2
#   Found even: 2
#   Found even: 4

for i in 1..3
    for j in 1..2
        echo "i=$i, j=$j"
    end
end

set n 1
while $n <= 5
    if $n == 2
        echo "Found even: $n"
    end
    if $n == 4
        echo "Found even: $n"
    end
    n = $n + 1
end
